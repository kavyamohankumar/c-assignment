Solution:

#include <iostream>
using namespace std;
int main()
{
  int arr[30], size, pos, i, insElem, count = 0;
  cout << "Enter the size of an array (Max size: 30): ";
  cin >> size;
  cout << "Enter array elements:\n";
  for (i = 0; i < size; i++)
    cin >> arr[i];

  cout << "\nEnter element to be inserted: ";
  cin >> insElem;

  cout << "Enter the position: "; 
  cin >> pos;

  for (i = ; i >= pos; i--)
    arr[] = arr[];
    arr[] insElem;

 
  cout << "New Array after Insertion:\n";
  for (i = 0; i < (size + 1); i++)
    cout << arr[i] << " ";

  return 0;
}


//Sonal

#include<iostream>
using namespace std;

int main()

{

    int w,h,a,p;

    cout<<"Enter  Width of Rectangle = ";

    cin>>w

    cout<<"Enter  Height of Rectangle = ";

    cin>>h;

    a=h*w;

    cout<<"Area of Rectangle ="<<a<<endl;

    p=2*(h+w);

cout<<" Perimeter of rectangle are = "<<p<<endl;

return 0;

}
