#include <stdlib.h>
using namespace std;
int main()
{
int a = 0,b = 0,i = 0, n;
char num[100];
cin>> num;
n = strlen(num);
while(n>0)
{
if(i==0)
{
a=a+num[n-1]-48;
n--;
i=1;
}
else
{
b=b+num[n-1]-48;
n--;
i=0;

