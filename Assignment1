//Ananya
Solution:

#include<iostream>
#include<string>
using namespace std;
int main()
{
    string str;
    cout<<"Enter the string"<<endl;
    cin>>str;
    for(size_t i=0;i<str.length();i++)
    {
        if(isupper(str[i]))
           {
               str[i]=tolower(str[i]);
           }
        else
        {
          str[i]=toupper(str[i]);
        }
    }
    cout<<str;
    return 0;
}

